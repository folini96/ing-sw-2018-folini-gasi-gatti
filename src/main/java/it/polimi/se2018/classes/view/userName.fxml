<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Pane prefHeight="275.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.polimi.se2018.classes.view.userNameController">
    <children>
        <VBox alignment="CENTER" layoutX="80.0" layoutY="79.0" prefHeight="118.0" prefWidth="160.0">
            <children>
                <Label prefHeight="17.0" prefWidth="160.0" text="Inserisci un username:" textAlignment="CENTER">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <TextField fx:id="userNameTextField">
                    <VBox.margin>
                        <Insets top="5.0" />
                    </VBox.margin>
                </TextField>
                <Button fx:id="userNameConfirmButton" mnemonicParsing="false" onAction="#userNameConfirmButtonClicked" text="Inizia">
                    <VBox.margin>
                        <Insets top="20.0" />
                    </VBox.margin>
                </Button>
            </children>
        </VBox>
    </children>
</Pane>
